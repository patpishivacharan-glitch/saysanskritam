{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/posters/*", "*.{css,js,json,svg,png,jpg,jpeg,gif,ico}"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".svg": "image/svg+xml"
  }
}
